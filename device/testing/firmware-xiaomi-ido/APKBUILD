pkgname=firmware-xiaomi-ido
pkgver=1
pkgrel=0
pkgdesc="Firmware files for Xiaomi Redmi 3 (ido)"
url="https://postmarketos.org"
arch="aarch64"
license="proprietary"
depends="wcnss-wlan alsa-ucm-conf firmware-qcom-adreno-a420 soc-qcom-msm8916"
options="!strip !tracedeps"

# Source
_files="mba.mbn modem.b00 modem.b01 modem.b02 modem.b03 modem.b04 modem.b05 modem.b06 modem.b08 modem.b09 modem.b12 modem.b13 modem.b14 modem.b15 modem.b16 modem.b17 modem.b18 modem.b21 modem.b22 modem.b23 modem.b25 modem.b26 modem.mdt wcnss.b00 wcnss.b01 wcnss.b02 wcnss.b04 wcnss.b06 wcnss.b09 wcnss.b10 wcnss.b11 wcnss.mdt venus.b00 venus.b01 venus.b02 venus.b03 venus.b04 venus.mbn venus.mdt"
_files_prima="WCNSS_cfg.dat WCNSS_qcom_cfg.ini WCNSS_qcom_wlan_nv.bin WCNSS_wlan_dictionary.dat"
_url="https://gitlab.com/reocat/redmi-3-proprietary/-/raw/main/lib"
_prima_url="https://gitlab.com/reocat/redmi-3-proprietary/-/raw/main/lib/wlan/prima"
for _i in $_files; do
	source="$source $_url/$_i"
done
for _i in $_files_prima; do
	source="$source $_prima_url/$_i"
done

package() {
	cd "$srcdir"

	# /lib/firmware/postmarketos
	for _i in $_files; do
		install -D -m644 "$_i" \
			"$pkgdir/lib/firmware/postmarketos/$_i"
	done

	# /lib/firmware/postmarketos/wlan/prima
	for _i in $_files_prima; do
		install -D -m644 "$_i" \
			"$pkgdir/lib/firmware/postmarketos/wlan/prima/$_i"
	done

}
